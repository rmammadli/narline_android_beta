      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="SeekBarNarManiaTheme" parent="android:Widget.SeekBar">
      <item name="android:progressDrawable">@drawable/narmaniatheme_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/narmaniatheme_scrubber_progress_horizontal_holo_light</item>
