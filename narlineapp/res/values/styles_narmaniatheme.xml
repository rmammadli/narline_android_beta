  </style>

  <style name="SpinnerDropDownItemNarManiaTheme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/narmaniatheme_btn_radio_holo_light</item>
  </style>
  <style name="TabNarManiaTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/narmaniatheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextNarManiaTheme">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarNarManiaTheme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/narmaniatheme_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/narmaniatheme_progress_indeterminate_horizontal_holo_light</item>
